<% layout("layout/boilerplate") %>
<head>
  <title>Student Portfolio|Takudzwa Maseva education </title>
</head>
    <!-- ------------Education Section---------------- -->
    <section id="education">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <h2 class="section-title mb-5 mt-5 text-center">My <span class="sub-color">Education</span></h2>
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div class="card border-0 p-5 mb-4 shadow">
                <div class="row">
                  <div class="col-md-3 d-flex align-items-center mb-3">
                    <img class="img-fluid" width="100px" height="100px" src="Images/stigs.png" alt="St Ignatius College Chishawasha logo">
                  </div>
                  <div class="col-md-9">
                    <h3 class="card-subtitle mb-2 dark-text">St Ignatius College</h3>
                    <p class="card-text text-muted mb-1">January 2015 - 2020</p>
